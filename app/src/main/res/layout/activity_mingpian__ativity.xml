<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mingpian__ativity"
    android:layout_width="match_parent"
    android:background="#f1f7f8"
    android:layout_height="match_parent"

    android:orientation="vertical"
    tools:context="com.example.yihuii.yihuii.MainFrame.frament1.mingpian_camera">

    <include layout="@layout/inicode" />

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_marginRight="15dp"
        android:layout_marginLeft="15dp"
        android:background="#f1f7f8"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
     <TextView
         android:textColor="#ccc"
         android:text="姓名"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_name"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="职位信息"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_zhiwei"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="公司信息"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_gongsi"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="邮箱地址"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_youxiang"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="手机"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_shouji"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="工作电话"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_dianhua"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="地址"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_dizhi"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" /> <TextView
         android:textColor="#ccc"
         android:text="网站"
         android:textSize="15sp"
         android:gravity="center_vertical"
         android:layout_width="match_parent"
         android:layout_height="40dp" />
        <EditText
            android:id="@+id/mingpian_edit_wangzhi"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:hint="暂无相关信息"
            android:background="@drawable/edit_backage"
            android:textColorHint="#ccc"
            android:layout_width="match_parent"
            android:layout_height="45dp" />
    </LinearLayout>
</ScrollView>
</LinearLayout>
